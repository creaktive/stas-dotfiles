ForwardAgent        yes
ForwardX11          no
TCPKeepAlive        yes
Compression         no
ServerAliveInterval 60
ServerAliveCountMax 10
ControlMaster       auto
ControlPath         /tmp/ssh_mux_%h_%p_%r
ControlPersist      4h
StrictHostKeyChecking yes
VerifyHostKeyDNS    yes

Host *
    AddKeysToAgent  yes
#    UseKeychain     yes
#    IdentityFile    ...
#    IdentityFile    ...

Host github.com
    IdentityFile    /Users/stan/.ssh/github

Host kindle
    User            root
    Hostname        kindle-4nt
    IdentityFile    /Users/stan/.ssh/kindle

Host mediabox
    User            pi
    Hostname        mediabox.sysd.org
    IdentityFile    /Users/stan/.ssh/openmediavault

Host sysd.org charles-carrol.dreamhost.com
    User            pusep
    IdentityFile    /Users/stan/.ssh/dreamhost
